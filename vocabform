 <script>  
  const scriptURL = 'https://script.google.com/macros/s/AKfycbwldu4COqJQfjjDLS5deyt9yWsPa8BfDQaCYa5X1IQ5nu6Ztc0Y/exec'  
  const form = document.forms['contact']  
  form.addEventListener('submit', e => {  
   e.preventDefault()  
   fetch(scriptURL, { method: 'POST', body: new FormData(form)})  
  //  .then(response => console.log('Successmmm!', response))  
  //  .catch(error => console.error('Error!', error.message))  
      .then(response => success())  
      .catch(error => console.error('Error!', error.message))  
  })  
  function success() {
  document.getElementById("status").innerHTML = "Form Submitted to Google sheet." + "<a href='https://docs.google.com/spreadsheets/d/1EEzGsAV3c8y16NCToSeVknnL9Hr4JUtXtqdsyPXJAJE/edit#gid=2044095440'target=_blank'>Click here</a> to access the Google sheet";
  
   clear();
 
  }
  
  function clear () {
  document.contact.Name.value='';
  document.contact.Book1.value='';
  document.contact.Author1.value='';
  document.contact.Book2.value='';
  document.contact.Author2.value='';
  document.contact.Book3.value='';
  document.contact.Author3.value='';
  document.contact.Book4.value='';
  document.contact.Author4.value='';
  document.contact.Book5.value='';
  document.contact.Author5.value='';
   
  }
  
  $(document).ready(function(){
    $("input").focus(function(){
        $(this).css("background-color", "#cccccc");
    });
    $("input").blur(function(){
        $(this).css("background-color", "#ffffff");
    });
});
 </script> 

